{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "f5ea20ea-4447-4a22-bc3d-e9c9165230a4",
            "level": 0,
            "email": "user@example.com"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "serverName": "v199.ohmyserver.ir",
          "certificates": [
            {
              "certificateFile": "/etc/v2ray/certs/fullchain.pem",
              "keyFile": "/etc/v2ray/certs/privkey.pem"
            }
          ],
          "alpn": ["h2", "http/1.1"]
        },
        "wsSettings": {
          "path": "/vmessws",
          "headers": {
            "Host": "v199.ohmyserver.ir"
          }
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "blocked"
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "blocked"
      }
    ]
  }
}
